            app:destination="@id/graphSecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/graphSecondFragment"
        android:name="com.example.practical_work_72.GraphSecondFragment"
        android:label="fragment_graph_second"
        tools:layout="@layout/fragment_graph_second" />
