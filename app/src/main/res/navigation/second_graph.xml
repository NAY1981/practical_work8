    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/second_graph"
    app:startDestination="@id/graphFirstFragment">

    <fragment
        android:id="@+id/graphFirstFragment"
        android:name="com.example.practical_work_72.GraphFirstFragment"
        android:label="fragment_graph_first"
        tools:layout="@layout/fragment_graph_first" >
        <action
